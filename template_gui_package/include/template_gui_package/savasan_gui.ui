   <property name="styleSheet">
    <string notr="true">/* General QTabWidget Styles */
QTabWidget::pane {
    border: 1px solid #3D3D3D;   /* Subtle border around the tab pane */
    background-color: #2B2B2B;   /* Dark background for the tab content area */
}

/* Tab Bar (where the tabs themselves sit) */
QTabBar::tab {
    background-color: #3D3D3D;   /* Dark grey background for inactive tabs */
    color: #A6A6A6;              /* Muted grey text for inactive tabs */
    padding: 8px 16px;           /* Padding for a modern, spacious look */
    border: 1px solid #2C2C2C;   /* Subtle border around tabs */
    border-bottom-color: #404EED;/* Bottom border to highlight selected tab */
    border-top-left-radius: 4px; /* Slight rounding for a modern feel */
    border-top-right-radius: 4px;/* Slight rounding for a modern feel */
    min-width: 100px;            /* Minimum width for uniform tabs */
    font-size: 14px;             /* Font size for the tab text */
    transition: background-color 0.2s ease, color 0.2s ease;
}

/* Hover Effect on Tabs */
QTabBar::tab:hover {
    background-color: #505050;   /* Slightly lighter grey when hovered */
    color: #EAEAEA;              /* Light grey text for hovered tabs */
}

/* Active (Selected) Tab */
QTabBar::tab:selected {
    background-color: #404EED;   /* Deep purple-blue for the active tab */
    color: #FFFFFF;              /* White text for active tab */
    border-bottom-color: #2B2B2B;/* Remove bottom border for active tab */
}

/* Disabled Tabs */
QTabBar::tab:disabled {
    background-color: #2C2C2C;   /* Darker grey for disabled tabs */
    color: #666666;              /* Muted text for disabled tabs */
}

/* Tab Bar Area */
QTabBar {
    background-color: #1E1E1E;   /* Dark background for the tab bar itself */
    border: none;                /* No border for a clean look */
}
</string>
