      <string notr="true">/* General QLineEdit and QSpinBox Styles */
QLineEdit, QSpinBox {
    background-color: #2B2B2B;    /* Dark grey background */
    color: #EAEAEA;               /* Light grey text */
    border: 1px solid #3D3D3D;    /* Subtle border for distinction */
    padding: 5px;                 /* Padding for a clean, modern look */
    border-radius: 4px;           /* Rounded corners for a modern feel */
    font-size: 14px;              /* Font size for the text */
}

/* Focused QLineEdit and QSpinBox */
QLineEdit:focus, QSpinBox:focus {
    border: 1px solid #404EED;    /* Deep purple-blue border when focused */
    background-color: #333333;    /* Slightly lighter background when focused */
}

/* Disabled QLineEdit and QSpinBox */
QLineEdit:disabled, QSpinBox:disabled {
    background-color: #1E1E1E;    /* Darker background for disabled state */
    color: #666666;               /* Muted text color for disabled fields */
    border: 1px solid #2C2C2C;    /* Subtle border for disabled state */
}

/* QSpinBox Arrow Buttons */
QSpinBox::up-button, QSpinBox::down-button {
    background-color: #3D3D3D;    /* Dark grey buttons */
    border: none;                 /* Remove border for a clean look */
    width: 16px;                  /* Set width of the buttons */
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: #505050;    /* Slightly lighter when hovered */
}

QSpinBox::up-arrow, QSpinBox::down-arrow {
    image: url(/path/to/up-arrow.png);  /* Add custom arrow icons if needed */
}</string>
