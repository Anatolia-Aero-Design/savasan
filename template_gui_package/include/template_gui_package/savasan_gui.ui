        <property name="styleSheet">
         <string notr="true">/* General QComboBox Styles */
QComboBox {
    background-color: #2B2B2B;    /* Dark grey background for the combo box */
    color: #EAEAEA;               /* Light grey text for readability */
    border: 1px solid #3D3D3D;    /* Subtle border for distinction */
    padding: 5px;                 /* Padding for a clean, modern look */
    border-radius: 4px;           /* Rounded corners for modern design */
    font-size: 14px;              /* Font size for text */
    combobox-popup: 0;            /* Prevent the popup from appearing inline */
}

/* Drop-down Arrow */
QComboBox::down-arrow {
    image: url(/path/to/down-arrow.png);  /* Custom down-arrow icon if needed */
    width: 12px;                 /* Set arrow size */
    height: 12px;
}

QComboBox::down-arrow:hover {
    image: url(/path/to/down-arrow-hover.png); /* Change icon on hover */
}

QComboBox::drop-down {
    background-color: #3D3D3D;    /* Dark grey background for the drop-down button */
    width: 24px;                  /* Width of the drop-down button */
    border-left: 1px solid #2C2C2C; /* Subtle left border to separate button */
}

/* Popup List (when expanded) */
QComboBox QAbstractItemView {
    background-color: #2B2B2B;    /* Dark grey background for the list */
    color: #EAEAEA;               /* Light grey text */
    selection-background-color: #404EED; /* Deep purple-blue for the selected item */
    selection-color: #FFFFFF;     /* White text for the selected item */
    border: 1px solid #3D3D3D;    /* Subtle border around the popup list */
}

/* Hover State */
QComboBox:hover {
    background-color: #505050;    /* Lighter grey when hovered */
    border: 1px solid #404EED;    /* Purple-blue border when hovered */
}

/* Disabled State */
QComboBox:disabled {
    background-color: #1E1E1E;    /* Darker grey background for disabled state */
    color: #666666;               /* Muted text color */
    border: 1px solid #2C2C2C;    /* Subtle border for disabled state */
}

/* Editable ComboBox Text */
QComboBox QLineEdit {
    background-color: #2B2B2B;    /* Dark grey background for the editable part */
    color: #EAEAEA;               /* Light grey text */
    border: none;                 /* Remove border from the editable area */
    padding: 5px;                 /* Padding for modern look */
}
